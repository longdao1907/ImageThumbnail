{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=your-cloud-sql-ip;Database=image-db;Username=your-user;Password=your-password;SslMode=Require;Trust Server Certificate=true"
  },
  "Gcp": {
    "BucketName": "your-gcs-bucket-name"
  },
  "Jwt": {
    "Issuer": "your-auth-api-issuer",
    "Audience": "your-image-api-audience",
    "Key": "your-super-secret-key-that-is-at-least-32-characters-long"
  }
}